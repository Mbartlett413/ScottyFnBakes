<p id="notice"><%= notice %></p>

<h1>Days Of Weeks</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Open</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @days_of_weeks.each do |days_of_week| %>
      <tr>
        <td><%= days_of_week.title %></td>
        <td><%= days_of_week.open %></td>
        <td><%= link_to 'Show', days_of_week %></td>
        <td><%= link_to 'Edit', edit_days_of_week_path(days_of_week) %></td>
        <td><%= link_to 'Destroy', days_of_week, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Days Of Week', new_days_of_week_path %>
